{
  "name": "Astro-bot",
  "version": "1.0.0",
  "description": "Advanced Discord Bot with AntiNuke, AutoMod & Utility Commands",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "pm2:start": "pm2 start index.js --name discord-bot",
    "pm2:stop": "pm2 stop discord-bot",
    "pm2:restart": "pm2 restart discord-bot",
    "pm2:logs": "pm2 logs discord-bot",
    "update": "npm update",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "discord",
    "bot",
    "antinuke",
    "automod",
    "moderation",
    "discord-js"
  ],
  "author": "TuZhi Codes",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "fs-extra": "^11.2.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "pm2": "^5.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuzhicodes/Astro-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/tuzhicodes/Astro-Bot/issues"
  },
  "homepage": "https://github.com/tuzhicodes/Astro-Bot#readme"
}
